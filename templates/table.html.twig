{% extends 'base.html.twig' %}

{% block body %}
    <table>
        <tbody>
        {% for record in records %}
            <tr>
                <td>{{ record.id }}</td>
                <td>{{ record.type }}</td>
                <td>{{ record.opt }}</td>
                <td>{{ record.displayHost }}</td>
                <td>{{ record.displayVal }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}